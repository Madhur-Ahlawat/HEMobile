<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph_landing"
    app:startDestination="@id/landingFragment">
    <fragment
        android:id="@+id/startNow"
        android:name="com.conduent.nationalhighways.ui.landing.StartNowFragment"
        android:label="@string/str_start_now"
        tools:layout="@layout/fragment_start_now" />
    <fragment
        android:id="@+id/viewChargesFragment"
        android:name="com.conduent.nationalhighways.ui.landing.ViewChargesFragment"
        android:label="@string/str_view_charges"
        tools:layout="@layout/fragment_view_charges" />
    <fragment
        android:id="@+id/contactDartChargeFragment"
        android:name="com.conduent.nationalhighways.ui.landing.ContactDartChargeFragment"
        android:label="@string/str_view_charges"
        tools:layout="@layout/fragment_contact_dart_charge2">
        <action
            android:id="@+id/action_contactDartChargeFragment_to_doYouHaveDartChargeFragment"
            app:destination="@id/doYouHaveDartChargeFragment" />
    </fragment>
    <fragment
        android:id="@+id/enquiryStatusFragment"
        android:name="com.conduent.nationalhighways.ui.bottomnav.account.raiseEnquiry.EnquiryStatusFragment"
        android:label="EnquiryStatusFragment"
        tools:layout="@layout/fragment_dart_charge_account_type_enquiry">
        <action
            android:id="@+id/action_enquiryStatusFragment_to_casesEnquiryDetailsFragment"
            app:destination="@id/casesEnquiryDetailsFragment" />
    </fragment>
    <fragment
        android:id="@+id/enquiryStatusDetailsFragment"
        android:name="com.conduent.nationalhighways.ui.landing.EnquiryStatusDetailsFragment"
        android:label="EnquiryStatusDetailsFragment"
        tools:layout="@layout/fragment_enquiry_status_details">
        <action
            android:id="@+id/action_enquiryStatusFragment_to_casesEnquiryDetailsFragment"
            app:destination="@id/casesEnquiryDetailsFragment" />
    </fragment>
    <fragment
        android:id="@+id/fragmentTermsAndConditions"
        android:name="com.conduent.nationalhighways.ui.landing.TermsAndConditionsFragment"
        android:label="Terms &amp; Conditions"
        tools:layout="@layout/fragment_terms_and_conditions" />
    <fragment
        android:id="@+id/doYouHaveDartChargeFragment"
        android:name="com.conduent.nationalhighways.ui.landing.DoYouHaveDartChargeFragment"
        android:label="Terms &amp; Conditions"
        tools:layout="@layout/fragment_do_you_have_dart_charge_account">
    </fragment>
    <fragment
        android:id="@+id/guidanceanddocumentsFragment"
        android:name="com.conduent.nationalhighways.ui.landing.GuidanceAndDocumentsFragment"
        android:label="Guidance and Documents"
        tools:layout="@layout/fragment_guidance_and_documents">

        <action
            android:id="@+id/action_guidanceanddocumentsFragment_to_contactDartChargeFragment"
            app:destination="@id/contactDartChargeFragment" />
        <action
            android:id="@+id/action_guidanceanddocumentsFragment_to_viewChargesFragment"
            app:destination="@id/viewChargesFragment" />
        <action
            android:id="@+id/action_guidanceanddocumentsFragment_to_aboutthisserviceFragment"
            app:destination="@id/aboutthisserviceFragment" />
        <action
            android:id="@+id/action_guidanceanddocumentsFragment_to_termsandconditions"
            app:destination="@id/fragmentTermsAndConditions" />
    </fragment>
    <fragment
        android:id="@+id/aboutthisserviceFragment"
        android:name="com.conduent.nationalhighways.ui.landing.AboutThisServiceFragment"
        android:label="Guidance and Documents"
        tools:layout="@layout/fragment_about_this_service" />

    <fragment
        android:id="@+id/landingFragment"
        android:name="com.conduent.nationalhighways.ui.landing.LandingFragment"
        android:label="@string/str_landing_screen"
        tools:layout="@layout/fragment_landing">

        <action
            android:id="@+id/action_landingFragment_to_guidanceanddocumentsFragment"
            app:destination="@id/guidanceanddocumentsFragment" />

        <action
            android:id="@+id/action_landingFragment_to_startNow"
            app:destination="@id/startNow" />
        <action
            android:id="@+id/action_landingFragment_to_serviceUnavailableFragment"
            app:destination="@id/serviceUnavailableFragment"
            app:popUpTo="@id/serviceUnavailableFragment"
            app:popUpToInclusive="true"/>
    </fragment>

    <fragment
        android:id="@+id/logoutFragment"
        android:name="com.conduent.nationalhighways.ui.landing.LogoutFragment"
        android:label="@string/str_log_out"
        tools:layout="@layout/fragment_logout">
        <action
            android:id="@+id/action_logoutFragment_to_startNow"
            app:destination="@id/landingFragment"
            app:popUpTo="@id/nav_graph_landing"
            app:popUpToInclusive="true" />
    </fragment>

    <fragment
        android:id="@+id/sessionTimeOutFragment"
        android:name="com.conduent.nationalhighways.ui.landing.SessionExpireFragment"
        android:label="@string/str_session_time_out"
        tools:layout="@layout/fragment_session_expire" />
    <fragment
        android:id="@+id/serviceUnavailableFragment"
        android:name="com.conduent.nationalhighways.ui.landing.ServiceUnavailableFragment"
        android:label="ServiceUnavailableFragment"
        tools:layout="@layout/fragment_service_unavailable" >
    </fragment>

    <fragment
        android:id="@+id/failedRetryFragment"
        android:name="com.conduent.nationalhighways.ui.landing.FailedRetryFragment"
        android:label="FailedRetryFragment"
        tools:layout="@layout/fragment_failed_retry" />

</navigation>