<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph_account_payment"
    app:startDestination="@id/accountPaymentHistoryFragment">

    <fragment
        android:id="@+id/accountPaymentHistoryFragment"
        android:name="com.heandroid.ui.bottomnav.account.payments.accountpaymenthistory.AccountPaymentHistoryFragment"
        android:label="AccountPaymentHistoryFragment"
        tools:layout="@layout/fragment_account_payment_history" >

        <action
            android:id="@+id/action_accountPaymentHistoryFragment_to_accountPaymentMethodsFragment"
            app:destination="@id/accountPaymentMethodsFragment" />
        <action
            android:id="@+id/action_accountPaymentHistoryFragment_to_accountTopUpPaymentFragment2"
            app:destination="@id/accountTopUpPaymentFragment" />
        <action
            android:id="@+id/action_accountPaymentHistoryFragment_to_accountPaymentHistoryItemDetailFragment"
            app:destination="@id/accountPaymentHistoryItemDetailFragment" />
        <action
            android:id="@+id/action_accountPaymentHistoryFragment_to_accountPaymentHistoryFilterFragment"
            app:destination="@id/accountPaymentHistoryFilterFragment" />
    </fragment>
    <fragment
        android:id="@+id/accountPaymentMethodsFragment"
        android:name="com.heandroid.ui.bottomnav.account.payments.accountpaymentmethod.AccountPaymentMethodsFragment"
        android:label="AccountPaymentMethodsFragment"
        tools:layout="@layout/fragment_account_payment_methods" >

        <action
            android:id="@+id/action_accountPaymentMethodsFragment_to_accountTopUpPaymentFragment2"
            app:destination="@id/accountTopUpPaymentFragment" />
        <action
            android:id="@+id/action_accountPaymentMethodsFragment_to_accountPaymentHistoryFragment"
            app:destination="@id/accountPaymentHistoryFragment" />
    </fragment>
    <fragment
        android:id="@+id/accountTopUpPaymentFragment"
        android:name="com.heandroid.ui.bottomnav.account.payments.accounttopup.AccountTopUpPaymentFragment"
        android:label="AccountTopUpPaymentFragment"
        tools:layout="@layout/fragment_account_topup_payment" >

        <action
            android:id="@+id/action_accountTopUpPaymentFragment_to_accountPaymentMethodsFragment2"
            app:destination="@id/accountPaymentMethodsFragment" />
        <action
            android:id="@+id/action_accountTopUpPaymentFragment_to_accountPaymentHistoryFragment2"
            app:destination="@id/accountPaymentHistoryFragment" />

        <action
            android:id="@+id/action_topupfragment_to_accountPaymentFragment"
            app:destination="@+id/accountFragmentParent"
            />

    </fragment>
    <fragment
        android:id="@+id/accountPaymentHistoryItemDetailFragment"
        android:name="com.heandroid.ui.bottomnav.account.payments.accountpaymenthistory.AccountPaymentHistoryItemDetailFragment"
        android:label="AccountPaymentHistoryItemDetailFragment"
        tools:layout="@layout/account_payment_history_item_detail"/>


    <fragment
        android:id="@+id/accountFragmentParent"
        android:name="com.heandroid.ui.bottomnav.account.AccountFragment"
        android:label="AccountFragment"
        tools:layout="@layout/fragment_account" />
    <fragment
        android:id="@+id/accountPaymentHistoryFilterFragment"
        android:name="com.heandroid.ui.bottomnav.account.payments.accountpaymenthistory.AccountPaymentHistoryFilterFragment"
        android:label="AccountPaymentHistoryFilterFragment"
        tools:layout="@layout/fragment_account_payment_history_filter"/>

</navigation>