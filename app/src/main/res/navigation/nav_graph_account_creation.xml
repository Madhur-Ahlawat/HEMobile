            android:id="@+id/action_createAccoutInfoFragment_to_createAccoutInfoConfirmationFragment"
            app:destination="@id/createAccoutInfoConfirmationFragment" />


    </fragment>

    <fragment
        android:id="@+id/createAccoutInfoConfirmationFragment"
        android:name="com.conduent.nationalhighways.ui.account.creation.step3.CreateAccountTopUpFragment"
        android:label="createAccoutInfoConfirmationFragment"
        tools:layout="@layout/fragment_create_account_top_up">
        <action
            android:id="@+id/action_createAccoutInfoConfirmationFragment_to_findYourVehicleFragment"
            app:destination="@id/findYourVehicleFragment" />
    </fragment>
    <!-- Step 4 -->
    <fragment
        android:id="@+id/createCommunicationPrefsFragment"
        android:name="com.conduent.nationalhighways.ui.account.creation.step4.CreateCommunicationPrefsFragment"
        android:label="createAccoutInfoConfirmationFragment"
        tools:layout="@layout/fragment_select_communication_preference">
        <action
            android:id="@+id/action_createCommunicationPrefsFragment_to_findYourVehicleFragment"
            app:destination="@id/findYourVehicleFragment" />


<!--
        <action
            android:id="@+id/action_createCommunicationPrefsFragment_to_businessVehicleTitleFragment"
            app:destination="@+id/businessVehicleTitleFragment" />
-->


    </fragment>


    <!-- Step 5 -->

    <fragment
        android:id="@+id/findYourVehicleFragment"
        android:name="com.conduent.nationalhighways.ui.account.creation.step5.CreateAccountFindVehicleFragment"
        android:label="FindYourVehicleFragment"
        tools:layout="@layout/fragment_create_account_find_vehicle">

<!--
        <action
            android:id="@+id/action_findYourVehicleFragment_to_businessVehicleTitleFragment"
            app:destination="@+id/businessVehicleTitleFragment" />
-->

        <action
            android:id="@+id/action_findVehicleFragment_to_businessVehicleUKListFragment"
            app:destination="@+id/businessVehicleUKListFragment"/>

        <action
            android:id="@+id/action_findVehicleFragment_to_businessVehicleNonUKMakeFragment"
            app:destination="@+id/businessNonUKMakeFragment"/>

        <action
            android:id="@+id/action_findYourVehicleFragment_to_businessVehicleDetailFragment"
            app:destination="@id/businessVehicleDetailFragment"
            />

    </fragment>

