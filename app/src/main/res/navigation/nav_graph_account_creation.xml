            app:destination="@id/accountTypeSelectionFragment" />
    </fragment>

    <fragment
        android:id="@+id/choosePaymentFragment"
        android:name="com.conduent.nationalhighways.ui.account.creation.step6.CreateAccountChoosePaymentFragment"
