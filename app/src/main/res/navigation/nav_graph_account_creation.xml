        tools:layout="@layout/fragment_business_info">

        <action
            android:id="@+id/action_businessInfoFragment_to_businessPrepayInfoFragment"
            app:destination="@id/businessPrepayInfoFragment" />
    </fragment>

    <fragment
        android:id="@+id/businessPrepayInfoFragment"
        android:name="com.conduent.nationalhighways.ui.account.creation.step2.businessaccount.BusinessPrePayInfoFragment"
