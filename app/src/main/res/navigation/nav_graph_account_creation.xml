        tools:layout="@layout/fragment_select_address">
        <action
            android:id="@+id/action_selectaddressfragment_to_createAccountEligibleLRDS2"
            app:destination="@id/createAccountEligibleLRDS" />
    </fragment>

    <fragment
        android:id="@+id/createAccountEligibleLRDS"
        tools:layout="@layout/fragment_create_account_eligible_lrds"
        android:name="com.conduent.nationalhighways.ui.account.creation.CreateAccountEligibleLRDS"
        android:label="CreateAccountEligibleLRDS" >
        <action
