        <action
            android:id="@+id/action_createAccountPrerequisite_to_createAccountPersonalInfo"
            app:destination="@id/createAccountPersonalInfo" />
    </fragment>
    <fragment
        android:id="@+id/createAccountPersonalInfo"
        android:name="com.conduent.nationalhighways.ui.account.creation.CreateAccountPersonalInfo"
        android:label="CreateAccountPersonalInfo"
        tools:layout="@layout/fragment_create_account_personal_info_new">
        <action
            android:id="@+id/action_createAccountPersonalInfo_to_createAccountPostCodeNew"
