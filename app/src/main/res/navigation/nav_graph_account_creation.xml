            app:destination="@+id/businessPrepayInfoFragment"/>
    </fragment>

    <fragment
        android:id="@+id/personalTypeFragment"
        android:name="com.conduent.nationalhighways.ui.account.creation.step2.CreateAccountPersonalSetupFragment"
        android:label="@string/str_select_account_type"
        tools:layout="@layout/fragment_create_account_personal_setup">

        <action
