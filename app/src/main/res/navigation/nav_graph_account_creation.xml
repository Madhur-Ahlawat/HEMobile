<?xml version="1.0" encoding="utf-8"?>
<navigation
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph_account_creation"
    app:startDestination="@id/createAccountPrerequisite">


   <!-- Step 1 -->
    <fragment
        android:id="@+id/emailVerification"
        android:name="com.conduent.nationalhighways.ui.account.creation.step1.CreateAccountEmailVerificationFragment"
        android:label="@string/str_email_verification"
        tools:layout="@layout/fragment_create_account_email_verification">
        <action
            android:id="@+id/action_emailVerification_to_confirmEmailFragment"
            app:destination="@id/confirmEmailFragment" />
    </fragment>


    <fragment
