<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph_nominated_contacts"
    app:startDestination="@id/ncNoListFragment">
    <fragment
        android:id="@+id/ncNoListFragment"
        android:name="com.conduent.nationalhighways.ui.nominatedcontacts.list.NoNominatedContactFragment"
        android:label="NoNominatedListFragment"
        tools:layout="@layout/fragment_no_nomiated_contact">
        <action
            android:id="@+id/action_ncNoListFragment_to_ncFullNameFragment"
            app:destination="@id/ncFullNameFragment"
            app:popUpTo="@id/ncFullNameFragment"
            app:popUpToInclusive="true" />
    </fragment>
    <fragment
        android:id="@+id/ncFullNameFragment"
        android:name="com.conduent.nationalhighways.ui.nominatedcontacts.invitation.NominatedInvitationFullNameFragment"
        android:label="NominatedEmailFragment"
        tools:layout="@layout/fragment_nominated_invitation_full_name">
        <action
            android:id="@+id/action_ncFullNameFragment_to_ncEmailFragment"
            app:destination="@id/ncEmailFragment"
            app:popUpTo="@id/ncEmailFragment"
            app:popUpToInclusive="true" />
    </fragment>
    <fragment
        android:id="@+id/ncEmailFragment"
        android:name="com.conduent.nationalhighways.ui.nominatedcontacts.invitation.NominatedInvitationContactFragment"
        android:label="NominatedEmailFragment"
        tools:layout="@layout/fragment_nominated_invitation_contact">
        <action
            android:id="@+id/action_ncEmailFragment_to_ncAcceessRightFragment"
            app:destination="@id/ncAcceessRightFragment" />
    </fragment>
    <fragment
        android:id="@+id/ncAcceessRightFragment"
        android:name="com.conduent.nationalhighways.ui.nominatedcontacts.invitation.NominatedInvitationAccessRightFragment"
        android:label="NominatedAccessRightFragment"
        tools:layout="@layout/fragment_nominated_invitation_access_right">
        <action
            android:id="@+id/action_ncAcceessRightFragment_to_ncListFragment"
            app:destination="@id/ncListFragment"
            app:popUpTo="@id/ncListFragment"
            app:popUpToInclusive="true" />

        <action
            android:id="@+id/action_ncAcceessRightFragment_to_ncFullNameFragment"
            app:destination="@id/ncFullNameFragment"
            app:popUpTo="@id/ncFullNameFragment"
            app:popUpToInclusive="true" />

    </fragment>
    <fragment
        android:id="@+id/ncListFragment"
        android:name="com.conduent.nationalhighways.ui.nominatedcontacts.list.NominatedContactListFragment"
        android:label="NominatedListFragment"
        tools:layout="@layout/fragment_nominated_contact_list">
        <action
            android:id="@+id/action_ncListFragment_to_ncFullNameFragment"
            app:destination="@id/ncFullNameFragment"
            app:popUpTo="@id/ncFullNameFragment"
            app:popUpToInclusive="true" />
        <action
            android:id="@+id/action_ncListFragment_to_ncNoListFragment"
            app:destination="@id/ncNoListFragment"
            app:popUpTo="@id/ncNoListFragment"
            app:popUpToInclusive="true" />
    </fragment>
</navigation>