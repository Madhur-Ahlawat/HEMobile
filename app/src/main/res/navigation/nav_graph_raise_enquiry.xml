<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph_raise_enquiry"
    app:startDestination="@id/enquiryCommentsFragment">
    <fragment
        android:id="@+id/thirdPartySoftwareFragment"
        android:name="com.conduent.nationalhighways.ui.landing.ThirdPartySoftwareFragment"
        android:label="thirdPartySoftwareFragment"
        tools:layout="@layout/fragment_third_party_software"/>
    <fragment
        android:id="@+id/aboutthisserviceFragment"
        android:name="com.conduent.nationalhighways.ui.landing.AboutThisServiceFragment"
        android:label="Guidance and Documents"
        tools:layout="@layout/fragment_about_this_service">
        <action
            android:id="@+id/action_aboutthisserviceFragment_to_viewChargesFragment"
            app:destination="@id/viewChargesFragment" />
    </fragment>
    <fragment
        android:id="@+id/viewChargesFragment"
        android:name="com.conduent.nationalhighways.ui.landing.ViewChargesFragment"
        android:label="@string/str_view_charges"
        tools:layout="@layout/fragment_view_charges" >
        <action
            android:id="@+id/action_contactDartChargeFragment_to_enquiryStatusFragment"
            app:destination="@id/enquiryStatusFragment" />
    </fragment>
    <fragment
        android:id="@+id/generalTermsAndConditionsFragment"
        android:name="com.conduent.nationalhighways.ui.landing.GeneralTermsAndConditionsFragment"
        android:label="EnquiryStatusFragment"
        tools:layout="@layout/fragment_general_terms_and_conditions">
        <action
            android:id="@+id/action_enquiryStatusFragment_to_enquiryStatusDetailsFragment"
            app:destination="@id/enquiryStatusDetailsFragment" />
    </fragment>
    <fragment
        android:id="@+id/privacyPolicyFragment"
        android:name="com.conduent.nationalhighways.ui.landing.PrivacyPolicyFragment"
        android:label="EnquiryStatusFragment"
        tools:layout="@layout/fragment_privacy_policy">
        <action
            android:id="@+id/action_enquiryStatusFragment_to_enquiryStatusDetailsFragment"
            app:destination="@id/enquiryStatusDetailsFragment" />
    </fragment>
    <fragment
        android:id="@+id/paygtermsandconditions"
        android:name="com.conduent.nationalhighways.ui.landing.PayGTermsAndConditionsFragment"
        android:label="EnquiryStatusFragment"
        tools:layout="@layout/fragment_payg_terms_and_conditions">
        <action
            android:id="@+id/action_enquiryStatusFragment_to_enquiryStatusDetailsFragment"
            app:destination="@id/enquiryStatusDetailsFragment" />
    </fragment>
    <fragment
        android:id="@+id/enquiryStatusDetailsFragment"
        android:name="com.conduent.nationalhighways.ui.landing.EnquiryStatusDetailsFragment"
        android:label="EnquiryStatusDetailsFragment"
        tools:layout="@layout/fragment_enquiry_status_details">
        <action
            android:id="@+id/action_enquiryStatusFragment_to_casesEnquiryDetailsFragment"
            app:destination="@id/casesEnquiryDetailsFragment" />
    </fragment>
    <fragment
        android:id="@+id/otherwaystopayFragment"
        android:name="com.conduent.nationalhighways.ui.landing.OtherWaysToPayFragment"
        android:label="EnquiryStatusDetailsFragment"
        tools:layout="@layout/fragment_other_ways_to_pay">
        <action
            android:id="@+id/action_otherwaystopayFragment_to_casesEnquiryDetailsFragment"
            app:destination="@id/casesEnquiryDetailsFragment" />
    </fragment>
    <fragment
        android:id="@+id/fragmentTermsAndConditions"
        android:name="com.conduent.nationalhighways.ui.landing.TermsAndConditionsFragment"
        android:label="Terms &amp; Conditions"
        tools:layout="@layout/fragment_terms_and_conditions">
        <action
            android:id="@+id/action_termsAndConditionsFragment_to_generalTermsAndConditions"
            app:destination="@id/generalTermsAndConditionsFragment" />
        <action
            android:id="@+id/action_termsAndConditionsFragment_to_privacyPolicyFragment"
            app:destination="@id/privacyPolicyFragment" />
        <action
            android:id="@+id/action_termsAndConditionsFragment_to_paygtermsandconditions"
            app:destination="@id/paygtermsandconditions" />

    </fragment>
    <fragment
        android:id="@+id/guidanceDocumentsFragment"
        android:name="com.conduent.nationalhighways.ui.bottomnav.account.raiseEnquiry.GuidanceDocumentsFragment"
        android:label="GuidanceDocumentsFragment"
        tools:layout="@layout/fragment_guidance_documents">
        <action
            android:id="@+id/action_guidanceDocumentsFragment_to_contactDartChargeFragment"
            app:destination="@id/contactDartChargeFragment" />
        <action
            android:id="@+id/action_guidanceDocumentsFragment_to_otherwaystopayFragment"
            app:destination="@id/otherwaystopayFragment" />
        <action
            android:id="@+id/action_guidanceanddocumentsFragment_to_thirdPartySoftwareFragment"
            app:destination="@id/thirdPartySoftwareFragment" />
        <action
            android:id="@+id/action_guidanceanddocumentsFragment_to_viewChargesFragment"
            app:destination="@id/viewChargesFragment" />
        <action
            android:id="@+id/action_guidanceanddocumentsFragment_to_aboutthisserviceFragment"
            app:destination="@id/aboutthisserviceFragment" />
        <action
            android:id="@+id/action_guidanceanddocumentsFragment_to_termsandconditions"
            app:destination="@id/fragmentTermsAndConditions" />
    </fragment>
    <fragment
        android:id="@+id/contactDartChargeFragment"
        android:name="com.conduent.nationalhighways.ui.landing.ContactDartChargeFragment"
        android:label="ContactDartChargeFragment"
        tools:layout="@layout/fragment_contact_dart_charge2">
        <action
            android:id="@+id/action_contactDartChargeFragment_to_dartChargeAccountTypeEnquiryFragment"
            app:destination="@id/dartChargeAccountTypeEnquiryFragment" />
        <action
            android:id="@+id/action_contactDartChargeFragment_to_enquiryStatusFragment"
            app:destination="@id/enquiryStatusFragment" />
    </fragment>
    <fragment
        android:id="@+id/enquiryStatusFragment"
        android:name="com.conduent.nationalhighways.ui.bottomnav.account.raiseEnquiry.EnquiryStatusFragment"
        android:label="EnquiryStatusFragment"
        tools:layout="@layout/fragment_dart_charge_account_type_enquiry">
        <action
            android:id="@+id/action_enquiryStatusFragment_to_casesEnquiryDetailsFragment"
            app:destination="@id/casesEnquiryDetailsFragment" />
    </fragment>


    <fragment
        android:id="@+id/dartChargeAccountTypeEnquiryFragment"
        android:name="com.conduent.nationalhighways.ui.bottomnav.account.raiseEnquiry.DartChargeAccountTypeEnquiryFragment"
        android:label="DartChargeAccountTypeEnquiryFragment"
        tools:layout="@layout/fragment_dart_charge_account_type_enquiry">
        <action
            android:id="@+id/action_dartChargeAccountTypeEnquiryFragment_to_enquiryCategoryFragment"
            app:destination="@id/enquiryCategoryFragment" />
    </fragment>

    <fragment
        android:id="@+id/enquiryCategoryFragment"
        android:name="com.conduent.nationalhighways.ui.bottomnav.account.raiseEnquiry.EnquiryCategoryFragment"
        android:label="EnquiryCategoryFragment"
        tools:layout="@layout/fragment_enquiry_category">
        <action
            android:id="@+id/action_enquiryCategoryFragment_to_enquiryCommentsFragment"
            app:destination="@id/enquiryCommentsFragment" />
        <action
            android:id="@+id/action_enquiryCategoryFragment_to_enquirySummaryFragment"
            app:destination="@id/enquirySummaryFragment"
            app:popUpTo="@id/enquirySummaryFragment"
            app:popUpToInclusive="true" />
    </fragment>
    <fragment
        android:id="@+id/enquiryCommentsFragment"
        android:name="com.conduent.nationalhighways.ui.bottomnav.account.raiseEnquiry.EnquiryCommentsFragment"
        android:label="EnquiryCommentsFragment"
        tools:layout="@layout/fragment_enquiry_comments">
        <action
            android:id="@+id/action_enquiryCommentsFragment_to_enquirySummaryFragment"
            app:destination="@id/enquirySummaryFragment"
            app:popUpTo="@id/enquirySummaryFragment"
            app:popUpToInclusive="true" />
        <action
            android:id="@+id/action_enquiryCommentsFragment_to_enquiryContactDetailsFragment"
            app:destination="@id/enquiryContactDetailsFragment" />
    </fragment>
    <fragment
        android:id="@+id/enquiryContactDetailsFragment"
        android:name="com.conduent.nationalhighways.ui.bottomnav.account.raiseEnquiry.EnquiryContactDetailsFragment"
        android:label="EnquiryContactDetailsFragment"
        tools:layout="@layout/fragment_enquiry_contact_details">
        <action
            android:id="@+id/action_enquiryContactDetailsFragment_to_enquirySummaryFragment"
            app:destination="@id/enquirySummaryFragment" />

        <action
            android:id="@+id/action_contactdetails_enquiryContactDetailsFragment_to_enquirySummaryFragment"
            app:destination="@id/enquirySummaryFragment"
            app:popUpTo="@id/enquirySummaryFragment"
            app:popUpToInclusive="true" />

        <action
            android:id="@+id/action_categoryChange_enquiryContactDetailsFragment_to_enquirySummaryFragment"
            app:destination="@id/enquirySummaryFragment"
            app:popUpTo="@id/enquirySummaryFragment"
            app:popUpToInclusive="true" />
        <action
            android:id="@+id/action_commentsChange_enquiryContactDetailsFragment_to_enquirySummaryFragment"
            app:destination="@id/enquirySummaryFragment"
            app:popUpTo="@id/enquirySummaryFragment"
            app:popUpToInclusive="true" />
    </fragment>
    <fragment
        android:id="@+id/enquirySummaryFragment"
        android:name="com.conduent.nationalhighways.ui.bottomnav.account.raiseEnquiry.SummaryEnquiryFragment"
        android:label="SummaryEnquiryFragment"
        tools:layout="@layout/fragment_summary_enquiry">
        <action
            android:id="@+id/action_enquirySummaryFragment_to_enquiryCategoryFragment"
            app:destination="@id/enquiryCategoryFragment" />
        <action
            android:id="@+id/action_enquirySummaryFragment_to_enquiryCommentsFragment"
            app:destination="@id/enquiryCommentsFragment" />
        <action
            android:id="@+id/action_enquirySummaryFragment_to_enquiryContactDetailsFragment"
            app:destination="@id/enquiryContactDetailsFragment" />
        <action
            android:id="@+id/action_enquirySummaryFragment_to_enquirySuccessFragment"
            app:destination="@id/enquirySuccessFragment" />
    </fragment>

    <fragment
        android:id="@+id/enquirySuccessFragment"
        android:name="com.conduent.nationalhighways.ui.bottomnav.account.raiseEnquiry.EnquirySuccessFragment"
        android:label="EnquirySuccessFragment"
        tools:layout="@layout/fragment_enquiry_success">

        <action
            android:id="@+id/action_enquirySuccessFragment_to_caseEnquiryHistoryListFragment"
            app:destination="@id/caseEnquiryHistoryListFragment" />
        <action
            android:id="@+id/action_enquirySuccessFragment_to_enquiryStatusFragment"
            app:destination="@id/enquiryStatusFragment" />

    </fragment>

    <fragment
        android:id="@+id/caseEnquiryHistoryListFragment"
        android:name="com.conduent.nationalhighways.ui.bottomnav.account.raiseEnquiry.CasesEnquiryHistoryListFragment"
        android:label="CasesEnquiryHistoryListFragment"
        tools:layout="@layout/fragment_cases_enquiry_history_list">
        <action
            android:id="@+id/action_caseEnquiryHistoryListFragment_to_casesEnquiryDetailsFragment"
            app:destination="@id/casesEnquiryDetailsFragment" />
        <action
            android:id="@+id/action_caseEnquiryHistoryListFragment_to_enquiryCategoryFragment"
            app:destination="@id/enquiryCategoryFragment" />


    </fragment>
    <fragment
        android:id="@+id/casesEnquiryDetailsFragment"
        android:name="com.conduent.nationalhighways.ui.bottomnav.account.raiseEnquiry.CasesEnquiryDetailsFragment"
        android:label="CasesEnquiryDetailsFragment"
        tools:layout="@layout/fragment_cases_enquiry_details">
    </fragment>

</navigation>