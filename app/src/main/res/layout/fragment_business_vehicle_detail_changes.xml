<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:paddingTop="@dimen/margin_18dp"
    android:paddingStart="@dimen/margin_20dp"
    android:paddingEnd="@dimen/margin_20dp">

    <com.conduent.nationalhighways.utils.widgets.NHTextView
        style="@style/MediumTextBlack"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:text="@string/str_is_this_your_vehicle_looking_for"
        android:id="@+id/isYourVehicle"

        />

    <androidx.cardview.widget.CardView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:cardCornerRadius="5dp"
        android:elevation="3dp"
        app:layout_constraintTop_toBottomOf="@id/isYourVehicle"
        android:layout_marginTop="15dp">
        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:padding="@dimen/margin_20dp">
            <com.conduent.nationalhighways.utils.widgets.NHTextView
                android:text="@string/vehicle_reg_number"
                style="@style/MediumTextBlack"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                android:id=""
                />



        </androidx.constraintlayout.widget.ConstraintLayout>

    </androidx.cardview.widget.CardView>



</androidx.constraintlayout.widget.ConstraintLayout>