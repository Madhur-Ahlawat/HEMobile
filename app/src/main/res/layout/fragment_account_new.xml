<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <androidx.core.widget.NestedScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:fillViewport="true">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/headerParent"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@color/white"
            android:importantForAccessibility="no">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/header"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/margin_20dp"
                android:layout_marginTop="@dimen/margin_25dp"
                android:importantForAccessibility="yes"
                android:screenReaderFocusable="true"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent">

                <com.conduent.nationalhighways.utils.widgets.NHTextView
                    android:id="@+id/profile_pic"
                    style="@style/SemiBoldText"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@drawable/skyblue_circle"
                    android:gravity="center"
                    android:importantForAccessibility="no"
                    android:minWidth="@dimen/margin_90dp"
                    android:minHeight="@dimen/margin_90dp"
                    android:textColor="@color/white"
                    android:textSize="@dimen/font_40sp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    tools:text="12" />

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintLeft_toRightOf="@id/profile_pic"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintTop_toTopOf="parent">

                    <com.conduent.nationalhighways.utils.widgets.NHTextView
                        android:id="@+id/value_name"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/margin_24dp"
                        android:layout_marginBottom="@dimen/margin_1dp"
                        android:fontFamily="@font/open_sans_semibold"
                        android:gravity="center"
                        android:importantForAccessibility="no"
                        android:textColor="@color/black"
                        android:textSize="@dimen/font_16sp"
                        app:layout_constraintLeft_toLeftOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        tools:text="@string/str_dummy_name" />

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/ll_account_number"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/margin_24dp"
                        app:layout_constraintLeft_toLeftOf="parent"
                        app:layout_constraintRight_toRightOf="parent"
                        app:layout_constraintTop_toBottomOf="@id/value_name"
                        android:visibility="gone">

                        <com.conduent.nationalhighways.utils.widgets.NHTextView
                            android:id="@+id/tv_account_number_heading"
                            android:layout_width="@dimen/margin_0dp"
                            android:layout_height="wrap_content"
                            android:fontFamily="@font/open_sans"
                            android:gravity="center_vertical"
                            android:importantForAccessibility="no"
                            android:text="@string/txt_account_number"
                            android:textColor="@color/label_color"
                            android:textSize="@dimen/font_14sp"
                            app:layout_constraintLeft_toLeftOf="parent"
                            app:layout_constraintRight_toLeftOf="@id/guideline1"
                            app:layout_constraintTop_toTopOf="parent" />


                        <androidx.constraintlayout.widget.Guideline
                            android:id="@+id/guideline1"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:importantForAccessibility="no"
                            android:orientation="vertical"
                            app:layout_constraintGuide_percent="0.45" />

                        <com.conduent.nationalhighways.utils.widgets.NHTextView
                            android:id="@+id/tv_account_number_value"
                            android:layout_width="@dimen/margin_0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/margin_5dp"
                            android:layout_marginEnd="@dimen/margin_10dp"
                            android:fontFamily="@font/open_sans"
                            android:gravity="center|left"
                            android:importantForAccessibility="no"
                            android:textColor="@color/label_color"
                            android:textSize="@dimen/font_14sp"
                            app:layout_constraintLeft_toRightOf="@id/guideline1"
                            app:layout_constraintRight_toRightOf="parent"
                            app:layout_constraintTop_toTopOf="parent"
                            tools:text="@string/app_name" />
                    </androidx.constraintlayout.widget.ConstraintLayout>

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/ll_account_status"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/margin_24dp"
                        android:layout_marginTop="@dimen/margin_2dp"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintLeft_toLeftOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/ll_account_number"
                        tools:visibility="gone">

                        <com.conduent.nationalhighways.utils.widgets.NHTextView
                            android:id="@+id/tv_account_status_heading"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:fontFamily="@font/open_sans"
                            android:gravity="center_vertical"
                            android:importantForAccessibility="no"
                            android:text="@string/txt_account_status"
                            android:textColor="@color/label_color"
                            android:textSize="@dimen/font_14sp"
                            app:layout_constraintLeft_toLeftOf="parent"
                            app:layout_constraintRight_toLeftOf="@id/guideline2"
                            app:layout_constraintTop_toTopOf="parent" />

                        <androidx.constraintlayout.widget.Guideline
                            android:id="@+id/guideline2"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:importantForAccessibility="no"
                            android:orientation="vertical"
                            app:layout_constraintGuide_percent="0.45" />

                        <RelativeLayout
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/margin_5dp"
                            app:layout_constraintLeft_toRightOf="@id/guideline2"
                            app:layout_constraintRight_toRightOf="parent"
                            app:layout_constraintTop_toTopOf="parent">

                            <com.google.android.material.card.MaterialCardView
                                android:id="@+id/card_indicator_account_status"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginRight="@dimen/margin_5dp"
                                android:background="@color/white"
                                android:importantForAccessibility="no"
                                app:cardCornerRadius="@dimen/margin_5dp"
                                app:cardElevation="@dimen/margin_5dp">

                                <com.conduent.nationalhighways.utils.widgets.NHTextView
                                    android:id="@+id/indicator_account_status"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:background="@color/transparent"
                                    android:fontFamily="@font/open_sans"
                                    android:importantForAccessibility="no"
                                    android:paddingLeft="@dimen/margin_5dp"
                                    android:paddingTop="@dimen/margin_1dp"
                                    android:paddingRight="@dimen/margin_5dp"
                                    android:paddingBottom="@dimen/margin_1dp"
                                    android:textSize="@dimen/font_14sp"
                                    android:textStyle="bold"
                                    tools:text="@string/open" />

                            </com.google.android.material.card.MaterialCardView>
                        </RelativeLayout>
                    </androidx.constraintlayout.widget.ConstraintLayout>

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/ll_account_number_largefont"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/margin_24dp"
                        android:layout_marginTop="@dimen/margin_5dp"
                        android:visibility="gone"
                        app:layout_constraintLeft_toLeftOf="parent"
                        app:layout_constraintRight_toRightOf="parent"
                        app:layout_constraintTop_toBottomOf="@id/value_name">

                        <com.conduent.nationalhighways.utils.widgets.NHTextView
                            android:id="@+id/tv_account_number_heading_largefont"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:fontFamily="@font/open_sans"
                            android:gravity="center_vertical"
                            android:importantForAccessibility="no"
                            android:text="@string/txt_account_number"
                            android:textColor="@color/label_color"
                            android:textSize="@dimen/font_14sp"
                            app:layout_constraintLeft_toLeftOf="parent"
                            app:layout_constraintRight_toRightOf="parent"
                            app:layout_constraintTop_toTopOf="parent" />


                        <com.conduent.nationalhighways.utils.widgets.NHTextView
                            android:id="@+id/tv_account_number_value_largefont"
                            android:layout_width="@dimen/margin_0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/margin_10dp"
                            android:fontFamily="@font/open_sans"
                            android:gravity="center|left"
                            android:importantForAccessibility="no"
                            android:textColor="@color/label_color"
                            android:textSize="@dimen/font_14sp"
                            app:layout_constraintLeft_toLeftOf="parent"
                            app:layout_constraintRight_toRightOf="parent"
                            app:layout_constraintTop_toBottomOf="@+id/tv_account_number_heading_largefont"
                            tools:text="@string/app_name" />
                    </androidx.constraintlayout.widget.ConstraintLayout>

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/ll_account_status_largefont"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/margin_24dp"
                        android:visibility="gone"
                        android:layout_marginTop="@dimen/margin_5dp"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintLeft_toLeftOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/ll_account_number_largefont">

                        <com.conduent.nationalhighways.utils.widgets.NHTextView
                            android:id="@+id/tv_account_status_heading_largefont"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:fontFamily="@font/open_sans"
                            android:gravity="center_vertical"
                            android:importantForAccessibility="no"
                            android:text="@string/txt_account_status"
                            android:textColor="@color/label_color"
                            android:textSize="@dimen/font_14sp"
                            app:layout_constraintLeft_toLeftOf="parent"
                            app:layout_constraintRight_toRightOf="parent"
                            app:layout_constraintTop_toTopOf="parent" />


                        <RelativeLayout
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            app:layout_constraintLeft_toLeftOf="parent"
                            app:layout_constraintTop_toBottomOf="@+id/tv_account_status_heading_largefont"
                            app:layout_constraintRight_toRightOf="parent">

                            <com.google.android.material.card.MaterialCardView
                                android:id="@+id/card_indicator_account_status_largefont"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginRight="@dimen/margin_5dp"
                                android:background="@color/white"
                                android:importantForAccessibility="no"
                                app:cardCornerRadius="@dimen/margin_5dp"
                                app:cardElevation="@dimen/margin_5dp">

                                <com.conduent.nationalhighways.utils.widgets.NHTextView
                                    android:id="@+id/indicator_account_status_largefont"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:background="@color/transparent"
                                    android:fontFamily="@font/open_sans"
                                    android:importantForAccessibility="no"
                                    android:paddingLeft="@dimen/margin_5dp"
                                    android:paddingTop="@dimen/margin_1dp"
                                    android:paddingRight="@dimen/margin_5dp"
                                    android:paddingBottom="@dimen/margin_1dp"
                                    android:textSize="@dimen/font_14sp"
                                    android:textStyle="bold"
                                    tools:text="@string/open" />

                            </com.google.android.material.card.MaterialCardView>
                        </RelativeLayout>
                    </androidx.constraintlayout.widget.ConstraintLayout>


                </androidx.constraintlayout.widget.ConstraintLayout>
            </androidx.constraintlayout.widget.ConstraintLayout>


            <com.google.android.material.card.MaterialCardView
                android:id="@+id/profile_management"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginHorizontal="@dimen/margin_20dp"
                android:layout_marginTop="@dimen/margin_32dp"
                android:background="@color/white"
                android:minHeight="@dimen/margin_55dp"
                app:cardCornerRadius="@dimen/margin_5dp"
                app:cardElevation="@dimen/margin_5dp"
                app:layout_constraintTop_toBottomOf="@id/header">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/cl_main"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_gravity="center"
                    android:background="@color/white">

                    <ImageView
                        android:id="@+id/left_icon"
                        android:layout_width="@dimen/margin_25dp"
                        android:layout_height="@dimen/margin_25dp"
                        android:layout_marginStart="@dimen/margin_17dp"
                        android:scaleType="centerCrop"
                        android:src="@drawable/ic_profile_management"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />

                    <com.conduent.nationalhighways.utils.widgets.NHTextView
                        android:id="@+id/value_title"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/margin_22dp"
                        android:fontFamily="@font/open_sans_semibold"
                        android:gravity="left"
                        android:text="@string/profile_management"
                        android:textColor="@color/black"
                        android:textSize="@dimen/font_15sp"
                        app:layout_constraintBottom_toBottomOf="@+id/left_icon"
                        app:layout_constraintEnd_toStartOf="@+id/icon_arrow"
                        app:layout_constraintStart_toEndOf="@+id/left_icon"
                        app:layout_constraintTop_toTopOf="@+id/left_icon" />


                    <ImageView
                        android:id="@+id/icon_arrow"
                        android:layout_width="@dimen/margin_16dp"
                        android:layout_height="@dimen/margin_16dp"
                        android:layout_marginStart="@dimen/margin_15dp"
                        android:layout_marginEnd="@dimen/margin_12dp"
                        android:src="@drawable/ic_right_purple"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />

                </androidx.constraintlayout.widget.ConstraintLayout>

            </com.google.android.material.card.MaterialCardView>

            <com.google.android.material.card.MaterialCardView
                android:id="@+id/communication_preferences"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginHorizontal="@dimen/margin_20dp"
                android:layout_marginTop="@dimen/margin_8dp"
                android:background="@color/white"
                android:minHeight="@dimen/margin_55dp"
                app:cardCornerRadius="@dimen/margin_5dp"
                app:cardElevation="@dimen/margin_5dp"
                app:layout_constraintTop_toBottomOf="@id/profile_management">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_gravity="center"
                    android:background="@color/white">

                    <ImageView
                        android:id="@+id/left_icon2"
                        android:layout_width="@dimen/margin_25dp"
                        android:layout_height="@dimen/margin_25dp"
                        android:layout_marginStart="@dimen/margin_17dp"
                        android:src="@drawable/ic_mail"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />

                    <com.conduent.nationalhighways.utils.widgets.NHTextView
                        android:id="@+id/value_title2"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/margin_22dp"
                        android:fontFamily="@font/open_sans_semibold"
                        android:gravity="left"
                        android:text="@string/communication_preferences"
                        android:textColor="@color/black"
                        android:textSize="@dimen/font_15sp"
                        app:layout_constraintBottom_toBottomOf="@+id/left_icon2"
                        app:layout_constraintEnd_toStartOf="@+id/icon_arrow2"
                        app:layout_constraintStart_toEndOf="@+id/left_icon2"
                        app:layout_constraintTop_toTopOf="@+id/left_icon2" />


                    <ImageView
                        android:id="@+id/icon_arrow2"
                        android:layout_width="@dimen/margin_16dp"
                        android:layout_height="@dimen/margin_16dp"
                        android:layout_marginStart="@dimen/margin_15dp"
                        android:layout_marginEnd="@dimen/margin_12dp"
                        android:src="@drawable/ic_right_purple"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />

                </androidx.constraintlayout.widget.ConstraintLayout>

            </com.google.android.material.card.MaterialCardView>

            <com.google.android.material.card.MaterialCardView
                android:id="@+id/payment_management"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginHorizontal="@dimen/margin_20dp"
                android:layout_marginTop="@dimen/margin_8dp"
                android:background="@color/white"
                android:minHeight="@dimen/margin_55dp"
                app:cardCornerRadius="@dimen/margin_5dp"
                app:cardElevation="@dimen/margin_5dp"
                app:layout_constraintTop_toBottomOf="@id/communication_preferences">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_gravity="center"
                    android:background="@color/white">

                    <ImageView
                        android:id="@+id/left_icon3"
                        android:layout_width="@dimen/margin_25dp"
                        android:layout_height="@dimen/margin_33dp"
                        android:layout_marginStart="@dimen/margin_17dp"
                        android:src="@drawable/ic_payment_management"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />

                    <com.conduent.nationalhighways.utils.widgets.NHTextView
                        android:id="@+id/value_title3"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/margin_22dp"
                        android:fontFamily="@font/open_sans_semibold"
                        android:gravity="left"
                        android:text="@string/payment_management"
                        android:textColor="@color/black"
                        android:textSize="@dimen/font_15sp"
                        app:layout_constraintBottom_toBottomOf="@+id/left_icon3"
                        app:layout_constraintEnd_toStartOf="@+id/icon_arrow3"
                        app:layout_constraintStart_toEndOf="@+id/left_icon3"
                        app:layout_constraintTop_toTopOf="@+id/left_icon3" />


                    <ImageView
                        android:id="@+id/icon_arrow3"
                        android:layout_width="@dimen/margin_16dp"
                        android:layout_height="@dimen/margin_16dp"
                        android:layout_marginStart="@dimen/margin_15dp"
                        android:layout_marginEnd="@dimen/margin_12dp"
                        android:src="@drawable/ic_right_purple"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />

                </androidx.constraintlayout.widget.ConstraintLayout>

            </com.google.android.material.card.MaterialCardView>

            <com.google.android.material.card.MaterialCardView
                android:id="@+id/vehicle_management"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginHorizontal="@dimen/margin_20dp"
                android:layout_marginTop="@dimen/margin_8dp"
                android:background="@color/white"
                android:minHeight="@dimen/margin_55dp"
                app:cardCornerRadius="@dimen/margin_5dp"
                app:cardElevation="@dimen/margin_5dp"
                app:layout_constraintTop_toBottomOf="@id/payment_management">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:background="@color/white">

                    <ImageView
                        android:id="@+id/left_icon4"
                        android:layout_width="@dimen/margin_25dp"
                        android:layout_height="@dimen/margin_25dp"
                        android:layout_marginStart="@dimen/margin_17dp"
                        android:src="@drawable/ic_car_blue"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />

                    <com.conduent.nationalhighways.utils.widgets.NHTextView
                        android:id="@+id/value_title4"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/margin_22dp"
                        android:fontFamily="@font/open_sans_semibold"
                        android:gravity="left"
                        android:text="@string/vehicle_management"
                        android:textColor="@color/black"
                        android:textSize="@dimen/font_15sp"
                        app:layout_constraintBottom_toBottomOf="@+id/left_icon4"
                        app:layout_constraintEnd_toStartOf="@+id/icon_arrow4"
                        app:layout_constraintStart_toEndOf="@+id/left_icon4"
                        app:layout_constraintTop_toTopOf="@+id/left_icon4" />


                    <ImageView
                        android:id="@+id/icon_arrow4"
                        android:layout_width="@dimen/margin_16dp"
                        android:layout_height="@dimen/margin_16dp"
                        android:layout_marginStart="@dimen/margin_15dp"
                        android:layout_marginEnd="@dimen/margin_12dp"
                        android:src="@drawable/ic_right_purple"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />

                </androidx.constraintlayout.widget.ConstraintLayout>
            </com.google.android.material.card.MaterialCardView>

            <com.google.android.material.card.MaterialCardView
                android:id="@+id/contact_us"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginHorizontal="@dimen/margin_20dp"
                android:layout_marginTop="@dimen/margin_8dp"
                android:background="@color/white"
                android:minHeight="@dimen/margin_55dp"
                app:cardCornerRadius="@dimen/margin_5dp"
                app:cardElevation="@dimen/margin_5dp"
                app:layout_constraintTop_toBottomOf="@id/vehicle_management">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_gravity="center"
                    android:background="@color/white">

                    <ImageView
                        android:id="@+id/left_icon5"
                        android:layout_width="@dimen/margin_25dp"
                        android:layout_height="@dimen/margin_25dp"
                        android:layout_marginStart="@dimen/margin_17dp"
                        android:src="@drawable/ic_voice_over_record"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />

                    <com.conduent.nationalhighways.utils.widgets.NHTextView
                        android:id="@+id/value_title5"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/margin_22dp"
                        android:fontFamily="@font/open_sans_semibold"
                        android:gravity="start"
                        android:text="@string/contact_us"
                        android:textColor="@color/black"
                        android:textSize="@dimen/font_15sp"
                        app:layout_constraintBottom_toBottomOf="@+id/left_icon5"
                        app:layout_constraintStart_toEndOf="@+id/left_icon5"
                        app:layout_constraintTop_toTopOf="@+id/left_icon5" />

                    <ImageView
                        android:id="@+id/icon_arrow5"
                        android:layout_width="@dimen/margin_16dp"
                        android:layout_height="@dimen/margin_16dp"
                        android:layout_marginStart="@dimen/margin_15dp"
                        android:layout_marginEnd="@dimen/margin_12dp"
                        android:src="@drawable/ic_right_purple"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />

                </androidx.constraintlayout.widget.ConstraintLayout>
            </com.google.android.material.card.MaterialCardView>

            <com.google.android.material.card.MaterialCardView
                android:id="@+id/close_acount"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginHorizontal="@dimen/margin_20dp"
                android:layout_marginTop="@dimen/margin_8dp"
                android:background="@color/white"
                android:minHeight="@dimen/margin_55dp"
                app:cardCornerRadius="@dimen/margin_5dp"
                app:cardElevation="@dimen/margin_5dp"
                app:layout_constraintTop_toBottomOf="@id/contact_us">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_gravity="center"
                    android:background="@color/white">

                    <ImageView
                        android:id="@+id/left_icon6"
