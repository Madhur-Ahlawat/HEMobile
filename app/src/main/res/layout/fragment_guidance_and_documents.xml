<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <androidx.appcompat.widget.LinearLayoutCompat
        android:orientation="vertical"
        android:backgroundTint="@color/white"
        android:background="@color/white"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <com.conduent.nationalhighways.utils.widgets.NHTextView
            android:layout_marginTop="@dimen/margin_17dp"
            android:layout_marginHorizontal="@dimen/margin_26dp"
            android:text="@string/dartford_crossing_is_a_free_flow_crossing"
            android:layout_width="match_parent"
            android:textSize="@dimen/font_14sp"
            android:textColor="@color/black"
            android:fontFamily="@font/open_sans"
            android:visibility="gone"
            android:layout_height="wrap_content" />

        <RelativeLayout
            android:id="@+id/layout_about_this_service"
            android:layout_marginHorizontal="@dimen/margin_26dp"
            android:background="@drawable/bg_rounded_corner_5dp"
            android:elevation="@dimen/margin_2dp"
            android:layout_marginTop="@dimen/margin_22dp"
            android:layout_width="match_parent"
            android:layout_height="@dimen/margin_62dp">

            <com.conduent.nationalhighways.utils.widgets.NHTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_centerVertical="true"
                android:layout_marginStart="@dimen/margin_21dp"
                android:fontFamily="@font/open_sans"
                android:text="About this service"
                android:textColor="@color/black"
                android:textSize="@dimen/font_14sp" />

            <ImageView
                android:id="@+id/btnAboutThisService"
                android:padding="@dimen/margin_19dp"
                android:src="@drawable/ic_right_arrow"
                android:layout_alignParentEnd="true"
                android:layout_width="@dimen/margin_50dp"
                android:layout_height="match_parent" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/layout_contact_dart_charge"
            android:layout_marginHorizontal="@dimen/margin_26dp"
            android:background="@drawable/bg_rounded_corner_5dp"
            android:elevation="@dimen/margin_2dp"
            android:layout_marginTop="@dimen/margin_9dp"
            android:layout_width="match_parent"
            android:layout_height="@dimen/margin_62dp">

            <com.conduent.nationalhighways.utils.widgets.NHTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_centerVertical="true"
                android:layout_marginStart="@dimen/margin_21dp"
                android:fontFamily="@font/open_sans"
                android:text="Contact Dart Charge"
                android:textColor="@color/black"
                android:textSize="@dimen/font_14sp" />

            <ImageView
                android:id="@+id/btnContactDartCharge"
                android:padding="@dimen/margin_19dp"
                android:src="@drawable/ic_right_arrow"
                android:layout_alignParentEnd="true"
                android:layout_width="@dimen/margin_50dp"
                android:layout_height="match_parent" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/layout_understanding_charges_and_fines_fines"
            android:layout_marginHorizontal="@dimen/margin_26dp"
            android:background="@drawable/bg_rounded_corner_5dp"
            android:elevation="@dimen/margin_2dp"
            android:layout_marginTop="@dimen/margin_9dp"
            android:layout_width="match_parent"
            android:layout_height="@dimen/margin_62dp">

            <com.conduent.nationalhighways.utils.widgets.NHTextView
                android:layout_width="wrap_content"
                android:text="Understanding Charges and Fines"
                android:textColor="@color/black"
                android:textSize="@dimen/font_14sp"
                android:fontFamily="@font/open_sans"
                android:layout_marginStart="@dimen/margin_21dp"
                android:layout_centerVertical="true"
                android:layout_alignParentStart="true"
                android:layout_height="wrap_content" />

            <ImageView
                android:id="@+id/btnUnderstandingChargesAndFines"
                android:padding="@dimen/margin_19dp"
                android:src="@drawable/ic_right_arrow"
                android:layout_alignParentEnd="true"
                android:layout_width="@dimen/margin_50dp"
                android:layout_height="match_parent" />
        </RelativeLayout>

        <RelativeLayout
            android:layout_marginHorizontal="@dimen/margin_26dp"
            android:background="@drawable/bg_rounded_corner_5dp"
            android:elevation="@dimen/margin_2dp"
            android:id="@+id/layout_termsAndConditions"
            android:layout_marginTop="@dimen/margin_9dp"
            android:layout_width="match_parent"
            android:layout_height="@dimen/margin_62dp">

            <com.conduent.nationalhighways.utils.widgets.NHTextView
                android:layout_width="wrap_content"
                android:text="Terms &amp; Conditions"
                android:textColor="@color/black"
                android:textSize="@dimen/font_14sp"
                android:fontFamily="@font/open_sans"
                android:layout_marginStart="@dimen/margin_21dp"
                android:layout_centerVertical="true"
                android:layout_alignParentStart="true"
                android:layout_height="wrap_content" />

            <ImageView
                android:id="@+id/btnTermsAndConditions"
                android:padding="@dimen/margin_19dp"
                android:src="@drawable/ic_right_arrow"
                android:layout_alignParentEnd="true"
                android:layout_width="@dimen/margin_50dp"
                android:layout_height="match_parent" />
        </RelativeLayout>

        <RelativeLayout
            android:layout_marginHorizontal="@dimen/margin_26dp"
            android:background="@drawable/bg_rounded_corner_5dp"
            android:elevation="@dimen/margin_2dp"
            android:layout_marginTop="@dimen/margin_9dp"
            android:layout_width="match_parent"
            android:layout_height="@dimen/margin_62dp">

            <com.conduent.nationalhighways.utils.widgets.NHTextView
                android:layout_width="wrap_content"
                android:text="Other ways to pay your charge"
                android:textColor="@color/black"
                android:textSize="@dimen/font_14sp"
                android:fontFamily="@font/open_sans"
                android:layout_marginStart="@dimen/margin_21dp"
                android:layout_centerVertical="true"
                android:layout_alignParentStart="true"
                android:layout_height="wrap_content" />

            <ImageView
                android:id="@+id/btnOtherWaysToPayCharge"
                android:padding="@dimen/margin_19dp"
                android:src="@drawable/ic_right_arrow"
                android:layout_alignParentEnd="true"
                android:layout_width="@dimen/margin_50dp"
                android:layout_height="match_parent" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/layout_third_party_software"
            android:layout_marginHorizontal="@dimen/margin_26dp"
            android:background="@drawable/bg_rounded_corner_5dp"
            android:elevation="@dimen/margin_2dp"
            android:layout_marginTop="@dimen/margin_9dp"
            android:layout_width="match_parent"
            android:layout_height="@dimen/margin_62dp">

            <com.conduent.nationalhighways.utils.widgets.NHTextView
                android:layout_width="wrap_content"
                android:text="Third party software"
                android:textColor="@color/black"
                android:textSize="@dimen/font_14sp"
                android:fontFamily="@font/open_sans"
                android:layout_marginStart="@dimen/margin_21dp"
                android:layout_centerVertical="true"
                android:layout_alignParentStart="true"
                android:layout_height="wrap_content" />

            <ImageView
                android:id="@+id/btnTHirdPartySoftware"
                android:padding="@dimen/margin_19dp"
                android:src="@drawable/ic_right_arrow"
                android:layout_alignParentEnd="true"
                android:layout_width="@dimen/margin_50dp"
                android:layout_height="match_parent" />
        </RelativeLayout>

        <com.conduent.nationalhighways.utils.widgets.NHButton
            android:id="@+id/btnFeedbackToImproveService"
            android:text="@string/feed_back_to_improve_service"
            style="@style/secondaryButton"
            android:textColorLink="@color/new_btn_color"
            android:textColor="@color/new_btn_color"
            app:borderColor="@color/new_btn_color"
            android:layout_marginTop="@dimen/margin_43dp"
            android:layout_marginHorizontal="@dimen/margin_26dp"
            android:layout_marginEnd="@dimen/margin_35dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />
    </androidx.appcompat.widget.LinearLayoutCompat>
</layout>