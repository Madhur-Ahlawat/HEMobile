<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <androidx.appcompat.widget.LinearLayoutCompat
        android:orientation="vertical"
        android:backgroundTint="@color/white"
        android:background="@color/white"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <com.conduent.nationalhighways.utils.widgets.NHTextView
            android:layout_marginTop="@dimen/_17dp"
            android:layout_marginHorizontal="@dimen/_26dp"
            android:text="@string/dartford_crossing_is_a_free_flow_crossing"
            android:layout_width="match_parent"
            android:textSize="@dimen/_14sp"
            android:textColor="@color/black"
            android:fontFamily="@font/open_sans"
            android:layout_height="wrap_content"/>
        <RelativeLayout
            android:id="@+id/layout_about_this_service"
            android:layout_marginHorizontal="@dimen/_26dp"
            android:background="@drawable/bg_rounded_corner_5dp"
            android:elevation="@dimen/_2dp"
            android:layout_marginTop="@dimen/_22dp"
            android:layout_width="match_parent"
            android:layout_height="@dimen/_62dp">

            <com.conduent.nationalhighways.utils.widgets.NHTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_centerVertical="true"
                android:layout_marginStart="@dimen/_21dp"
                android:fontFamily="@font/open_sans"
                android:text="About this service"
                android:textColor="@color/black"
                android:textSize="@dimen/_14sp" />
            <ImageView
                android:id="@+id/btnAboutThisService"
                android:padding="@dimen/_19dp"
                android:src="@drawable/ic_right_arrow"
                android:layout_alignParentEnd="true"
                android:layout_width="50dp"
                android:layout_height="match_parent"/>
        </RelativeLayout>
        <RelativeLayout
            android:id="@+id/layout_contact_dart_charge"
            android:layout_marginHorizontal="@dimen/_26dp"
            android:background="@drawable/bg_rounded_corner_5dp"
            android:elevation="@dimen/_2dp"
            android:layout_marginTop="@dimen/_9dp"
            android:layout_width="match_parent"
            android:layout_height="@dimen/_62dp">

            <com.conduent.nationalhighways.utils.widgets.NHTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_centerVertical="true"
                 android:layout_marginStart="@dimen/_21dp"
                android:fontFamily="@font/open_sans"
                android:text="Contact Dart Charge"
                android:textColor="@color/black"
                android:textSize="@dimen/_14sp" />
            <ImageView
                android:id="@+id/btnContactDartCharge"
                android:padding="@dimen/_19dp"
                android:src="@drawable/ic_right_arrow"
                android:layout_alignParentEnd="true"
                android:layout_width="50dp"
                android:layout_height="match_parent"/>
        </RelativeLayout>
        <RelativeLayout
            android:id="@+id/layout_understanding_charges_and_fines_fines"
            android:layout_marginHorizontal="@dimen/_26dp"
            android:background="@drawable/bg_rounded_corner_5dp"
            android:elevation="@dimen/_2dp"
            android:layout_marginTop="@dimen/_9dp"
            android:layout_width="match_parent"
            android:layout_height="@dimen/_62dp">
            <com.conduent.nationalhighways.utils.widgets.NHTextView
                android:layout_width="wrap_content"
                android:text="Understanding Charges and Fines"
                android:textColor="@color/black"
                android:textSize="@dimen/_14sp"
                android:fontFamily="@font/open_sans"
                 android:layout_marginStart="@dimen/_21dp"
                android:layout_centerVertical="true"
                android:layout_alignParentStart="true"
                android:layout_height="wrap_content"/>
            <ImageView
                android:id="@+id/btnUnderstandingChargesAndFines"
                android:padding="@dimen/_19dp"
                android:src="@drawable/ic_right_arrow"
                android:layout_alignParentEnd="true"
                android:layout_width="50dp"
                android:layout_height="match_parent"/>
        </RelativeLayout>
        <RelativeLayout
            android:layout_marginHorizontal="@dimen/_26dp"
            android:background="@drawable/bg_rounded_corner_5dp"
            android:elevation="@dimen/_2dp"
            android:id="@+id/layout_termsAndConditions"
            android:layout_marginTop="@dimen/_9dp"
            android:layout_width="match_parent"
            android:layout_height="@dimen/_62dp">
            <com.conduent.nationalhighways.utils.widgets.NHTextView
                android:layout_width="wrap_content"
                android:text="Terms &amp; Conditions"
                android:textColor="@color/black"
                android:textSize="@dimen/_14sp"
                android:fontFamily="@font/open_sans"
                 android:layout_marginStart="@dimen/_21dp"
                android:layout_centerVertical="true"
                android:layout_alignParentStart="true"
                android:layout_height="wrap_content"/>
            <ImageView
                android:id="@+id/btnTermsAndConditions"
                android:padding="@dimen/_19dp"
                android:src="@drawable/ic_right_arrow"
                android:layout_alignParentEnd="true"
                android:layout_width="50dp"
                android:layout_height="match_parent"/>
        </RelativeLayout>
        <RelativeLayout
            android:layout_marginHorizontal="@dimen/_26dp"
            android:background="@drawable/bg_rounded_corner_5dp"
            android:elevation="@dimen/_2dp"
            android:layout_marginTop="@dimen/_9dp"
            android:layout_width="match_parent"
            android:layout_height="@dimen/_62dp">
            <com.conduent.nationalhighways.utils.widgets.NHTextView
                android:layout_width="wrap_content"
                android:text="Other ways to pay your charge"
                android:textColor="@color/black"
                android:textSize="@dimen/_14sp"
                android:fontFamily="@font/open_sans"
                 android:layout_marginStart="@dimen/_21dp"
                android:layout_centerVertical="true"
                android:layout_alignParentStart="true"
                android:layout_height="wrap_content"/>
            <ImageView
                android:id="@+id/btnOtherWaysToPayCharge"
                android:padding="@dimen/_19dp"
                android:src="@drawable/ic_right_arrow"
                android:layout_alignParentEnd="true"
                android:layout_width="50dp"
                android:layout_height="match_parent"/>
        </RelativeLayout>
        <RelativeLayout
            android:id="@+id/layout_third_party_software"
            android:layout_marginHorizontal="@dimen/_26dp"
            android:background="@drawable/bg_rounded_corner_5dp"
            android:elevation="@dimen/_2dp"
            android:layout_marginTop="@dimen/_9dp"
            android:layout_width="match_parent"
            android:layout_height="@dimen/_62dp">
            <com.conduent.nationalhighways.utils.widgets.NHTextView
                android:layout_width="wrap_content"
                android:text="Third party software"
                android:textColor="@color/black"
                android:textSize="@dimen/_14sp"
                android:fontFamily="@font/open_sans"
                 android:layout_marginStart="@dimen/_21dp"
                android:layout_centerVertical="true"
                android:layout_alignParentStart="true"
                android:layout_height="wrap_content"/>
            <ImageView
                android:id="@+id/btnTHirdPartySoftware"
                android:padding="@dimen/_19dp"
                android:src="@drawable/ic_right_arrow"
                android:layout_alignParentEnd="true"
                android:layout_width="50dp"
                android:layout_height="match_parent"/>
        </RelativeLayout>
        <com.conduent.nationalhighways.utils.widgets.NHButton
            android:id="@+id/btnFeedbackToImproveService"
            android:text="FEEDBACK TO IMPROVE SERVICE"
            style="@style/secondaryButton"
            android:layout_marginTop="@dimen/_43dp"
            android:layout_marginHorizontal="@dimen/_26dp"
            android:layout_marginEnd="@dimen/_35dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"/>
    </androidx.appcompat.widget.LinearLayoutCompat>
</layout>