                android:id="@+id/fragment_dashboard_account_crossing_card"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_4sdp"
                app:cardBackgroundColor="@color/white"
                app:cardCornerRadius="@dimen/_2sdp"
                app:layout_constraintEnd_toEndOf="@id/glEnd"
                app:layout_constraintStart_toStartOf="@id/glStart"
                app:layout_constraintTop_toBottomOf="@id/fragment_dashboard_account_topup_card">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/view_crossings"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:minHeight="@dimen/_55sdp">

                    <View
                        android:id="@+id/fragment_dashboard_account_crossing_card_dummy"
                        android:layout_width="@dimen/_4sdp"
                        android:layout_height="0dp"
                        android:background="#457C1F"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintLeft_toLeftOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />

                    <View
                        android:id="@+id/fragment_dashboard_account_crossing_card_image_dummy"
                        android:layout_width="@dimen/_30sdp"
                        android:layout_height="@dimen/_30sdp"
                        android:layout_marginStart="@dimen/_10sdp"
                        android:background="@drawable/circular_shape_tint"
                        android:backgroundTint="#457C1F"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintLeft_toRightOf="@id/fragment_dashboard_account_crossing_card_dummy"
                        app:layout_constraintTop_toTopOf="parent" />

                    <androidx.appcompat.widget.AppCompatImageView
                        android:layout_width="0dp"
                        android:layout_height="0dp"
                        android:padding="@dimen/_8sdp"
                        android:src="@drawable/ic_crossing"
                        app:layout_constraintBottom_toBottomOf="@id/fragment_dashboard_account_crossing_card_image_dummy"
                        app:layout_constraintLeft_toLeftOf="@id/fragment_dashboard_account_crossing_card_image_dummy"
                        app:layout_constraintRight_toRightOf="@id/fragment_dashboard_account_crossing_card_image_dummy"
                        app:layout_constraintTop_toTopOf="@id/fragment_dashboard_account_crossing_card_image_dummy" />

                    <androidx.appcompat.widget.AppCompatTextView
                        android:id="@+id/tv_crossing_heading"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/_8sdp"
                        android:layout_marginTop="@dimen/_8sdp"
                        android:gravity="center_vertical"
                        android:text="@string/txt_your_crossings"
                        android:textColor="@android:color/black"
                        android:textSize="@dimen/_14ssp"
                        android:textStyle="bold"
                        app:layout_constraintLeft_toRightOf="@id/fragment_dashboard_account_crossing_card_image_dummy"
                        app:layout_constraintTop_toTopOf="parent" />

                    <TextView
                        android:id="@+id/tv_crossing_count"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/_8sdp"
                        android:text="@string/str_two_crossing"
                        android:textColor="@color/btn_disable_txt_color"
                        android:textSize="@dimen/_14ssp"
                        app:layout_constraintLeft_toRightOf="@id/fragment_dashboard_account_crossing_card_image_dummy"
                        app:layout_constraintTop_toBottomOf="@id/tv_crossing_heading" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_1sdp"
                        android:includeFontPadding="false"
                        android:text="@string/txt_view_crossings"
                        android:textColor="@color/blue_color"
                        android:textSize="@dimen/_13ssp"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintRight_toLeftOf="@id/your_crossing_arrow_image"
                        app:layout_constraintTop_toTopOf="parent" />

                    <androidx.appcompat.widget.AppCompatImageView
                        android:id="@+id/your_crossing_arrow_image"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginRight="10dp"
                        android:rotation="180"
                        android:src="@drawable/ic_back_arrow"
                        android:tint="@color/color_99999"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintRight_toRightOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />

                </androidx.constraintlayout.widget.ConstraintLayout>

            </com.google.android.material.card.MaterialCardView>

            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/_20sdp"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintTop_toBottomOf="@id/fragment_dashboard_account_vehicle_card" />

            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/glStart"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical"
                app:layout_constraintGuide_begin="@dimen/_10sdp" />

            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/glEnd"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical"
                app:layout_constraintGuide_end="@dimen/_10sdp" />

        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.core.widget.NestedScrollView>

</layout>
